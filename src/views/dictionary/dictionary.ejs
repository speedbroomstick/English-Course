<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>DarkEnglish</title>
    <link rel="shortcut icon" href="/favicon.gif" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="/css/novbar.css" />
    <link rel="stylesheet" href="/css/app.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <script src="/socket.io/socket.io.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
  </head>

  <body style="background-image: url('/fon_main.jpg')">
    <%- include('../main/header'); -%>
      <div class="wrapper_area" id="dictionary">
        <div class="ag-format-container" style="width: 100%;">
          <table class="table">
            <thead id="table2">
              <tr>
                <th scope="col">Word</th>
                <th scope="col">Translation</th>
                <th scope="col">Example</th>
                <th scope="col"><span class="add_word"><ion-icon id="add_words"
                      name="add-circle-sharp"></ion-icon></span></th>
              </tr>
            </thead>
            <tbody id="table">
              <% words.forEach((word)=> { %>
                <%- include('tableWords', {word:word}); -%>
                  <% });%>
            </tbody>
          </table>
        </div>
      </div>
      <%- include('../other/pagination', {count:count}); -%>
      <%- include('../other/panelAuthorization'); -%>
      <script type="module" src="/js/authorization.js"></script>
      <script src="/js/app.js"></script>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.min.js" integrity="sha384-Y4oOpwW3duJdCWv5ly8SCFYWqFDsfob/3GkgExXKV4idmbt98QcxXYs9UoXAB7BZ" crossorigin="anonymous"></script>
      
  </body>

</html>